        <input id="aName" placeholder="Product Name" class="w-full mb-2 px-3 py-2 rounded bg-slate-800 border border-slate-600">
        <input id="aPrice" type="number" placeholder="Price (€)" class="w-full mb-2 px-3 py-2 rounded bg-slate-800 border border-slate-600">
        
        <select id="aCategory" class="w-full mb-2 px-3 py-2 rounded bg-slate-800 border border-slate-600">
