}

function loginAdmin() {
    if (document.getElementById("adminPass").value === ADMIN_PASSWORD) {
        document.getElementById("adminLogin").classList.add("hidden");
        document.getElementById("adminPanel").classList.remove("hidden");
        renderAdminList();
    } else {
        alert("Wrong password");
    }
}

// SAVE / UPDATE
function saveProduct() {
    const product = {
        name: aName.value,
        price: Number(aPrice.value),
        category: aCategory.value,
        image: aImage.value,
        link: aLink.value,
        bestSeller: aBest.checked
    };

    if (!product.name || !product.price) {
        alert("Name & Price required");
        return;
    }

    if (editIndex !== null) {
        adminProducts[editIndex] = product;
        editIndex = null;
    } else {
        adminProducts.push(product);
    }

    localStorage.setItem("adminProducts", JSON.stringify(adminProducts));
    alert("Saved ✅");
    location.reload();
}

// DELETE
function deleteProduct(index) {
    if (!confirm("Delete product?")) return;
    adminProducts.splice(index, 1);
    localStorage.setItem("adminProducts", JSON.stringify(adminProducts));
    location.reload();
}

// EDIT
function editProduct(index) {
    const p = adminProducts[index];
    aName.value = p.name;
    aPrice.value = p.price;
    aCategory.value = p.category;
    aImage.value = p.image;
    aLink.value = p.link;
    aBest.checked = p.bestSeller;
    editIndex = index;
}

// LIST
function renderAdminList() {
    const list = document.getElementById("productList");
    list.innerHTML = "";

    adminProducts.forEach((p, i) => {
        list.innerHTML += `
        <div class="flex justify-between items-center bg-slate-800 p-2 rounded">
            <span class="truncate">${p.name}</span>
            <div class="flex gap-2">
                <button onclick="editProduct(${i})" class="text-yellow-400">✏️</button>
                <button onclick="deleteProduct(${i})" class="text-red-500">🗑️</button>
            </div>
        </div>`;
    });
}
</script>

</html>