    <button onclick="toggleAdmin()" class="bg-yellow-500 text-black px-4 py-2 rounded-full font-bold shadow-lg">
        ADMIN
    </button>

    <div id="adminBox" class="hidden mt-4 w-80 bg-slate-900 border border-slate-700 rounded-xl p-4 text-sm">
