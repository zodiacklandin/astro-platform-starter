        </select>

        <input id="aImage" placeholder="Image path / URL" class="w-full mb-2 px-3 py-2 rounded bg-slate-800 border border-slate-600">
        <input id="aLink" placeholder="Buy Link" class="w-full mb-3 px-3 py-2 rounded bg-slate-800 border border-slate-600">

        <label class="flex items-center gap-2 mb-3">
            <input id="aBest" type="checkbox">
            Best Seller
        </label>

        <button onclick="addProduct()" class="w-full bg-yellow-500 text-black py-2 rounded font-bold">
            ADD PRODUCT
        </button>
    </div>
</div>
<script>
function toggleAdmin() {
    document.getElementById("adminBox").classList.toggle("hidden");
}

function addProduct() {
    const product = {
        name: document.getElementById("aName").value,
        price: Number(document.getElementById("aPrice").value),
        category: document.getElementById("aCategory").value,
        image: document.getElementById("aImage").value,
        link: document.getElementById("aLink").value,
        bestSeller: document.getElementById("aBest").checked
    };

    if (!product.name || !product.price) {
        alert("Name & Price required");
        return;
    }

    const stored = JSON.parse(localStorage.getItem("adminProducts")) || [];
    stored.push(product);
    localStorage.setItem("adminProducts", JSON.stringify(stored));

    alert("Product added ✅");
    location.reload();
}
</script>
