        // --- RENDER LOGIC ---

        function createCard(product) {
            return `
                <div class="product-card bg-slate-800 rounded-xl overflow-hidden border border-slate-700 relative group animate-fade-in-up flex flex-col h-full">
                    <div class="absolute top-3 left-3 z-20 flex gap-2">
                        ${product.bestSeller ? '<span class="bg-yellow-500 text-black text-[10px] font-bold px-2 py-1 rounded uppercase tracking-wider shadow-lg">Best Seller</span>' : ''}
                    </div>
                    
                    <div class="h-64 md:h-72 overflow-hidden relative bg-slate-900 flex-shrink-0">
                        <img src="${product.image}" alt="${product.name}" class="w-full h-full object-cover transform group-hover:scale-110 transition duration-700" onerror="this.onerror=null; this.src='https://via.placeholder.com/300x400?text=No+Image';">
                        
                        <!-- Hover Overlay with BUY NOW button -->
                        <div class="absolute inset-0 bg-black/70 opacity-0 group-hover:opacity-100 transition duration-300 flex items-center justify-center backdrop-blur-sm z-30">
                            <a href="${product.link}" target="_blank" class="bg-yellow-500 text-black px-8 py-3 rounded-full font-bold hover:bg-white hover:text-black transition transform translate-y-4 group-hover:translate-y-0 shadow-xl uppercase text-sm tracking-wide">
                                Buy Now
                            </a>
                        </div>
                    </div>

                    <div class="p-5 flex-grow flex flex-col justify-between relative bg-cardbg">
                        <div>
                            <div class="text-[10px] text-gray-400 uppercase tracking-widest mb-2 font-bold opacity-60">${product.category}</div>
                            <!-- Product Name acts as a link too -->
                            <a href="${product.link}" target="_blank" class="hover:text-yellow-400 transition block">
                                <h3 class="font-bold text-lg mb-1 text-white leading-tight">${product.name}</h3>
                            </a>
                        </div>
                        <div class="flex justify-between items-end mt-4 border-t border-slate-700 pt-4">
                            <div class="flex flex-col">
                                <span class="text-xs text-gray-500 line-through mb-0.5 text-gray-400">$${Math.floor(product.price * 1.3)}</span>
                                <span class="text-yellow-400 font-extrabold text-2xl">$${product.price}</span>
                            </div>
                            <button class="text-gray-400 hover:text-red-500 transition">
                                <i class="far fa-heart"></i>
                            </button>
                        </div>
                    </div>
                </div>
            `;
        }

        function renderProducts(filter = 'all', searchTerm = '') {
            const productContainer = document.getElementById('productGrid');
            const noResults = document.getElementById('noResults');
            const bestSellerHeader = document.getElementById('bestSellerHeader');

            // Copy array to avoid mutating original
            let displayList = [...products];

            // Filter by Category
            if (filter !== 'all') {
                if (filter === 'clothing') {
                     // Group hoodies, t-shirts, jackets into "clothing"
                     displayList = displayList.filter(p => p.category === 'clothing');
                } else {
                    displayList = displayList.filter(p => p.category === filter);
                }
            }

            // Filter by Search Term
            if (searchTerm) {
                displayList = displayList.filter(p => p.name.toLowerCase().includes(searchTerm.toLowerCase()));
            }

            // Sort: Best Sellers first (only if no specific sort applied)
            if (filter === 'all' && !searchTerm) {
                displayList.sort((a, b) => (b.bestSeller === a.bestSeller) ? 0 : b.bestSeller ? -1 : 1);
                if(bestSellerHeader) bestSellerHeader.classList.remove('hidden');
            } else {
                if(bestSellerHeader) bestSellerHeader.classList.add('hidden');
            }

            // Render
            if (displayList.length === 0) {
                productContainer.innerHTML = '';
                noResults.classList.remove('hidden');
            } else {
                noResults.classList.add('hidden');
                productContainer.innerHTML = displayList.map(createCard).join('');
            }
        }

        // --- EVENT LISTENERS ---

        const searchInput = document.getElementById('searchInput');
        searchInput.addEventListener('input', (e) => {
            const term = e.target.value;
            const activeBtn = document.querySelector('.category-btn.active');
            const activeCategory = activeBtn ? getCategoryFromBtn(activeBtn) : 'all';
            renderProducts(activeCategory, term);
        });

        function filterProducts(category, btnElement) {
            if (btnElement) {
                document.querySelectorAll('.category-btn').forEach(btn => btn.classList.remove('active'));
                btnElement.classList.add('active');
            } else {
                // Default reset
                document.querySelectorAll('.category-btn').forEach(btn => btn.classList.remove('active'));
                const firstBtn = document.querySelector('.category-btn');
                if(firstBtn) firstBtn.classList.add('active');
            }

            const term = document.getElementById('searchInput').value;
            renderProducts(category, term);
        }

        function getCategoryFromBtn(btn) {
            return btn.innerText.toLowerCase();
        }

        // Initialize
        window.addEventListener('DOMContentLoaded', () => {
            renderProducts();
        });

    </script>
</body>
